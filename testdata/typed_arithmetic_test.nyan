meow test_typed_add() {
  expect(add(1, 2), 3)
  expect(add(-1, 1), 0)
}

meow test_typed_sub() {
  expect(sub(5, 3), 2)
  expect(sub(1, 5), -4)
}

meow test_typed_mul() {
  expect(mul(3, 4), 12)
  expect(mul(-2, 3), -6)
}

meow test_typed_div() {
  expect(div(10, 2), 5)
  expect(div(7, 2), 3)
}

meow test_typed_abs() {
  expect(abs(5), 5)
  expect(abs(-3), 3)
  expect(abs(0), 0)
}

meow test_typed_fib() {
  expect(fib(0), 0)
  expect(fib(1), 1)
  expect(fib(10), 55)
}

meow test_typed_max_val() {
  expect(max_val(3, 5), 5)
  expect(max_val(10, 2), 10)
}

meow test_typed_concat() {
  expect(concat("Hello", ", World!"), "Hello, World!")
}

meow test_typed_is_positive() {
  judge(is_positive(1))
  refuse(is_positive(0))
  refuse(is_positive(-1))
}
