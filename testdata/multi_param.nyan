# Multiple parameter functions

meow add3(a int, b int, c int) int {
  bring a + b + c
}

meow add4(a int, b int, c int, d int) int {
  bring a + b + c + d
}

meow clamp(val int, lo int, hi int) int {
  sniff (val < lo) {
    bring lo
  }
  sniff (val > hi) {
    bring hi
  }
  bring val
}

meow noop() {
  catnap
}

meow identity(x int) int {
  bring x
}
